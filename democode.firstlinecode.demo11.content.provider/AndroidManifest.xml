        android:minSdkVersion="18"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity android:name = "democode.firstlinecode.demo11.content.provider.main.MainActivity" >
            
            <intent-filter>
                
                <action   android:name = "android.intent.action.MAIN" />
                
                <category android:name = "android.intent.category.LAUNCHER" />
                
            </intent-filter>
            
        </activity>
        
        <activity
            android:name = "democode.firstlinecode.demo11.content.provider.contacts.ReadSystemContactsActivity"
             >
            
        </activity>
        
        <activity
            android:name = "democode.firstlinecode.demo11.content.provider.custom.MyContentProviderActivity"
             >
            
        </activity>
        
    </application>

</manifest>
